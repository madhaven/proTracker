{
  "name": "protracker",
  "version": "1.0.0",
  "description": "app to track tasks",
  "main": "./Scripts/eMain.js",
  "scripts": {
    "build": "cd pUIng && npm run build",
    "buildnstart": "cd pUIng && npm run build && cd .. && npm run start",
    "start": "electron .",
    
    "debug": "electron . --inspect=5858",
    "test": "echo \"Error: no test specified\" && exit 1",
    
    "clean": "del proTracker*.db appconfig.json proTracker.log",
    "cleandebug": "del proTracker*.db appconfig.json proTracker.log && npm run debug",

    "make": "electron-forge make",
    "package": "electron-forge package",
    
    "postinstall": "npm install ./pUIng/package.json",
    "install:pUIng": "cd pUIng && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/madhaven/proTracker.git"
  },
  "keywords": [
    "habit",
    "track",
    "task"
  ],
  "author": "madhaven",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/madhaven/proTracker/issues"
  },
  "homepage": "https://github.com/madhaven/proTracker#readme",
  "devDependencies": {
    "@electron-forge/cli": "^6.4.1",
    "@electron-forge/maker-deb": "^6.2.1",
    "@electron-forge/maker-rpm": "^6.2.1",
    "@electron-forge/maker-squirrel": "^6.2.1",
    "@electron-forge/maker-zip": "^6.2.1",
    "@electron-forge/plugin-auto-unpack-natives": "^6.2.1",
    "electron": "^26.2.4"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "exceljs": "^4.3.0",
    "fast-csv": "^4.3.6",
    "sqlite3": "^5.1.6"
  }
}
